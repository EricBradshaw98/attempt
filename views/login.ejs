  <script defer src="/scripts/app.js"></script>
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css" rel="stylesheet">
    <title>Login</title>
  </head>

      <body>
        <%- include('partials/_header') %>
        <main class="column-container">
          <div class="card">
          <div class="card-header"></div>
          <div class="card-body">
            <h5 class="card-title"></h5>
            <form method="POST" action="/login">
              <div class="form-group">
                <label for="newEmail">Email:
                  <input type=email class="form-control" id="newEmail" name="email" ></label>
                  <label for="newPassword">Password:
                    <input type=password class="form-control" id="newPassword" name="password" ></label>

                  </div>
              <button type="submit" class="btn btn-primary">Login</button>
            </form>
          </div>
        </div>


      </main>
  </body>
</html>
